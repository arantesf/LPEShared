                            <ColumnDefinition Width="*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Border BorderBrush="Black" BorderThickness="1" Background="#FF444444">
                            <TextBlock  Margin="10,0,0,0" Foreground="White" VerticalAlignment="Center" >ALTURA ESPAÇADOR SOLDADO PARA BARRA DE TRANSFERÊNCIA
                            </TextBlock>
                        </Border>
                        <Border BorderBrush="Black" BorderThickness="1" Background="White" Grid.Column="1">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="3*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <TextBox BorderThickness="0"></TextBox>
                                <TextBlock Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center">cm</TextBlock>
                            </Grid>
                        </Border>
                    </Grid>
                    <Grid Height="16" Margin="3">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="2*"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Border BorderBrush="Black" BorderThickness="1" Background="#FF444444">
                            <TextBlock Margin="10,0,0,0" Foreground="White" VerticalAlignment="Center" >ALTURA ESPAÇADORES PARA TELA INFERIOR
                            </TextBlock>
                        </Border>
                        <Border BorderBrush="Black" BorderThickness="1" Background="White" Grid.Column="1">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="3*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <TextBox BorderThickness="0"></TextBox>
                                <TextBlock Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center">cm</TextBlock>
                            </Grid>
                        </Border>
                    </Grid>
                </StackPanel>
            </Grid>
        </Grid>
        <Grid
            Grid.Row="1"
            Margin="10,0"
            >
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="200"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
                <ColumnDefinition Width="200"></ColumnDefinition>
            </Grid.ColumnDefinitions>

            <Button 
                x:Name="Execute_Button"
                Content="SALVAR AMBIENTE"
                Grid.Column="2">
                <Button.Style>
                    <Style TargetType="{x:Type Button}">
                        <Setter Property="Background" Value="#FF3b4453"/>
                        <Setter Property="BorderBrush" Value="Black"/>
                        <Setter Property="Foreground" Value="White"/>
                        <Setter Property="FontWeight" Value="SemiBold"/>
                        <Style.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Background" Value="#FF000000"/>
                                <Setter Property="Foreground" Value="#FF0e538c"/>
                            </Trigger>
                        </Style.Triggers>
                    </Style>
                </Button.Style>
            </Button>
            <Button 
                x:Name="SelectPisos_Button"
                Content="EDITAR TIPO DE SOLUÇÃO"
                Grid.Column="0"
                >
                <Button.Style>
                    <Style TargetType="{x:Type Button}">
                        <Setter Property="BorderBrush" Value="Black"/>
                        <Style.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Background" Value="#FF000000"/>
                                <Setter Property="Foreground" Value="#FF0e538c"/>
                            </Trigger>
                        </Style.Triggers>
                    </Style>
                </Button.Style>
            </Button>

        </Grid>
        <Grid
            Margin="10"
            Grid.Row="4"
            >
            <ProgressBar x:Name="ProgressBar"
                         DataContext="{Binding ProgressBarViewModel, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                         Background="#FFDDDDDD"
                         Foreground="#FF2995ff"
                         Height="30" 
                         HorizontalAlignment="Stretch"
                         Minimum="0"
                         Maximum="{Binding Path=ProgressBarMaxValue}"
                         Value="{Binding Path=ProgressBarValue}"
                         VerticalAlignment="Center"
                         Width="Auto">

            </ProgressBar>
            <TextBlock
                Name="ProgressBar_TextBlock"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                >
            </TextBlock>
        </Grid>
    </Grid>
</Window>
